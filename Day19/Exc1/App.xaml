<Application x:Class="Exc1.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converters="clr-namespace:Exc1.Converters"
             StartupUri="Views/LoginWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
            <converters:NullToBooleanConverter x:Key="NullToBooleanConverter" IsInverted="False" />
            <converters:NullToBooleanConverter x:Key="NullToBooleanConverterInverted" IsInverted="True" />
            <converters:TypeToBoolConverter x:Key="TypeToBoolConverter" />
            <converters:BalanceToBrushConverter x:Key="BalanceToBrushConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>